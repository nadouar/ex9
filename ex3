package calcul;

import java.util.Scanner;

public class calculAB {

	public static void main(String[] args) {

		Scanner sc =new Scanner(System.in) ;
		System.out.println("donner a ");
		int a = sc.nextInt();
		System.out.println("donner b");
		int b = sc.nextInt();
		int max = a;
		if (b>a)
			max = b;
		System.out.println(max);
	}
}
